{
    "name": "darkplusplus",
    "publisher": "codetin",
    "displayName": "Dark++",
    "description": "Dark++ theme for vscode",
    "version": "0.2.6",
    "license": "MIT",
    "icon": "darkplusplus.png",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Themes"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/codetin/DarkPlusPlus.git"
    },
    "contributes": {
        "themes": [
            {
                "label": "Dark++",
                "uiTheme": "vs-dark",
                "path": "./themes/dark++-color-theme.json"
            }
        ]
    },
    "configurationDefaults": {
        "[Dark++]": {
            "editor.semanticHighlighting.enabled": true
        }
    },
    "__metadata": {
        "publisherDisplayName": "code_tin"
    }
