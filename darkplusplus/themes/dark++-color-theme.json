    "tokenColors": [
        {
            "name": "Operators",
            "scope": ["keyword.operator"],
            "settings": {
                "foreground": "#ee68af"
            }
        },
        {
            "name": "Comment",
            "scope": ["comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6A9955"
            }
        },
        // {
        //     "scope": "comment",
        //     "settings": {
        //         "foreground": "#6A9955"
        //     }
        // },
        {
            "name": "String",
            "scope": [
                "string",
                "constant.other.symbol",
                "entity.name.operator.custom-literal.string",
                "meta.embedded.assembly"
            ],
            "settings": {
                //"foreground": "#00ff0d"
                "foreground": "#95fb79"
            }
        },
        // {
        //     "scope": ["string", "entity.name.operator.custom-literal.string", "meta.embedded.assembly"],
        //     "settings": {
        //         "foreground": "#95fb79"
        //     }
        // },
        {
            "name": "Regular Expressions and Escape Characters",
            "scope": ["string.regexp", "constant.character", "constant.other"],
            "settings": {
                "foreground": "#9fe7fa"
            }
        },
        {
            "name": "Number",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#9ffcea"
            }
        },
