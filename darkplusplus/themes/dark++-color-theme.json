        },
        {
            "name": "Function call",
            "scope": ["variable.function", "variable.annotation", "meta.function-call.generic", "support.function.go"],
            "settings": {
                "foreground": "#fced65"
            }
        },
        {
            "name": "Library function",
            "scope": ["support.function", "support.macro"],
            "settings": {
                "foreground": "#fced65"
            }
        },
        {
            "name": "Library function console",
            "scope": ["support.function.console"],
            "settings": {
                "foreground": "#fced65"
            }
        },
        {
            "scope": ["meta.embedded", "source.groovy.embedded"],
            "settings": {
                "foreground": "#D4D4D4"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "header",
            "settings": {
                "foreground": "#000080"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": ["entity.name.operator.custom-literal.number", "variable.other.enummember"],
            "settings": {
                "foreground": "#91afd1"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#646695"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "entity.name.tag.css",
